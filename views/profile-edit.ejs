<!DOCTYPE html>
<html lang="es">
<!-- Include Head -->
	<%- include('partials/head')  %> 

<body>
	<%- include('partials/headerLogueado') %> 
	<!-- Header -->
	
	<!-- /Header -->

	<!-- Novedades -->
	<div class="container products-wrapper">
		<div class="row">
			<div class="col-12">
				<h2 class="products-title">Editar mi perfil</h2>
			</div>
			<!-- Show the "ultimos" products -->
			<form class="form-register" action="/users/profile" method="get">
				<div>
					<label> Cambiar Email:</label>
					<input type="email" name="email" value="">
				</div>
				<div>
					<label>Cambiar Usuario:</label>
					<input type="text" name="usuario" value="">
				</div>
				<div>
					<label>Cambiar Contraseña:</label>
					<input type="password" name="contraseña" value="">
				</div>
				<div>
					<label>Cambiar Fecha de Nacimiento:</label>
					<input type="date" name="fecha" value="">
				</div>
