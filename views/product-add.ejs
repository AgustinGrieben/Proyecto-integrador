<!DOCTYPE html>
<html lang="es">
<!-- Include Head -->
<%- include('partials/head') %> 
<body>

	<!-- Header -->
		<%- include('partials/headerLogueado') %> 
	<!-- /Header -->

	<!-- Novedades -->
	<div class="container products-wrapper">
		<div class="row">
			<div class="col-12">
				<h2 class="products-title">Agregar productos</h2>
			</div>
			<!-- Show the "ultimos" products -->
			
			<form class="form-register" action="/" method="get">
				<div>
					<label>Imagen:</label>
					<input type="file" name="imagen" value="" required>
				</div>
				<div>
					<label>Nombre del Producto:</label>
					<input type="text" name="Nombre del Producto" value="" required>
				</div>
				<div>
					<label>Descripcion:</label>
					<input type="text" name="Descripcion" value="" required>
				</div>
				<div>
					<label>Fecha de carga:</label>
					<input type="date" name="Fecha de carga" value="" required>
				</div>
				<button type="submit">Enviar</button>
			</form>

	<!-- footer -->
	<%- include('./partials/footer')  %> 
	<!-- /footer -->
</body>

</html>